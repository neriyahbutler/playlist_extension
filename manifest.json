{
    "name": "Music Playlist",
    "version": "1.0.0",
    "manifest_version": 2,
    
    "description": "Demo for music playlist",
    "icons": {"1000": "icon2.png"},
    
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["popup.js"]
        }
    ],
    "browser_action": {
        "default_icon":"icon2.png",
        "browser_title": "ANTI-WEEB Extension",
        "default_popup": "musicChoice.html"
    },
    
    "background": {
        "matches": ["<all_urls>"],
        "js": ["popup.js"]
    },
    "permissions": ["activeTab", "tabs", "storage"]
}